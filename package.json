{
  "name": "@deboxsoft/lc-webapp",
  "version": "1.0.1",
  "description": "Web Application Learning Center.",
  "private": true,
  "scripts": {
    "dev": "run-p routify nollup",
    "dev:ssr": "run-p routify rollup",
    "build": "cross-env NODE_ENV=production yarn build:app",
    "build:app": "routify -b && rollup -c",
    "build:static": "spank",
    "format": "prettier --write --plugin-search-dir ./ src/**/*.*",
    "serve": "spassr --ssr",
    "rollup": "rollup -cw",
    "nollup": "nollup -c",
    "routify": "routify",
    "typecheck:lint": "eslint --fix \"**/*.{ts,tsx,js,jsx}\"",
    "typecheck:svelte": "svelte-check",
    "typecheck:tsc": "tsc --noEmit",
    "typecheck": "run-p typecheck:svelte"
  },
  "dependencies": {
    "@deboxsoft/accounting-api": "^0.1.0-alpha.6",
    "@deboxsoft/accounting-client": "^0.1.0-alpha.7",
    "@deboxsoft/module-client": "^1.5.19-0",
    "@deboxsoft/module-core": "^1.5.19-0",
    "@deboxsoft/module-graphql": "^1.5.19-0",
    "@deboxsoft/module-validation": "^1.0.4",
    "@deboxsoft/svelte-components-base": "^0.0.12",
    "@deboxsoft/svelte-core": "canary",
    "@deboxsoft/svelte-forms": "canary",
    "@deboxsoft/svelte-icons": "^0.0.4",
    "@deboxsoft/svelte-theme-limitless": "canary",
    "@popperjs/core": "2.6.0",
    "@roxi/routify": "^2.9.0"
  },
  "devDependencies": {
    "@nurdiansyah/cpx": "1.5.0",
    "@nurdiansyah/devel": "^5.1.0",
    "@nurdiansyah/eslint-config-devel": "^7.1.0",
    "@nurdiansyah/rollup": "^1.3.0",
    "@nurdiansyah/stylelint-rscss": "^1.0.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@sucrase/jest-plugin": "^2.0.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@types/jest": "^24.0.11",
    "@types/nanoid": "^2.1.0",
    "@types/nconf": "^0.10.0",
    "@types/node": "*",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.8.0",
    "cross-env": "^5.2.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "fs-extra": "9.0.1",
    "husky": "^4.3.0",
    "hygen": "^6.0.4",
    "jest": "^26.0.0",
    "jest-dom": "^4.0.0",
    "jsdom": "^16.4.0",
    "lint-staged": "^10.5.1",
    "nollup": "^0.14.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.1",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^1.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "rollup-plugin-hot": "^0.1.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte-hot": "^0.13.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^5.2.1",
    "sass": "1.30.0",
