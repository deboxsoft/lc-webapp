{
  "name": "@deboxsoft/lc-webapp",
  "version": "1.0.1",
  "description": "Web Application Learning Center.",
  "private": true,
  "scripts": {
    "dev": "run-p routify nollup",
    "dev:ssr": "run-p routify rollup",
    "build": "cross-env NODE_ENV=production yarn build:app",
    "build:app": "routify -b && rollup -c",
    "build:static": "spank",
    "format": "prettier --write --plugin-search-dir ./ src/**/*.*",
    "serve": "spassr --ssr",
    "rollup": "rollup -cw",
    "nollup": "nollup -c",
    "routify": "routify",
    "lint:eslint": "eslint --fix \"**/*.{ts,tsx,js,jsx}\"",
    "lint:svelte": "svelte-check ./",
    "lint:tsc": "tsc --noEmit",
    "lint": "run-p lint:svelte lint:eslint lint:tsc"
  },
  "dependencies": {
    "@deboxsoft/accounting-api": "~0.2.0-0",
    "@deboxsoft/accounting-client": "~0.2.0-0",
    "@deboxsoft/module-client": "~1.8.0-0",
    "@deboxsoft/module-core": "~1.8.0-0",
    "@deboxsoft/module-graphql": "~1.8.0-0",
    "@deboxsoft/svelte-components-base": "^0.0.12",
    "@deboxsoft/svelte-core": "^0.0.0-9",
    "@deboxsoft/svelte-forms": "^0.0.3-19",
    "@deboxsoft/svelte-icons": "^0.0.4",
    "@deboxsoft/svelte-theme-limitless": "^0.0.6-5",
    "@deboxsoft/svelte-virtual-list": "^1.6.7",
    "@deboxsoft/users-api": "~1.2.0-0",
    "@deboxsoft/users-client": "~1.2.0-0",
    "@deboxsoft/zod": "^3.0.0-0",
    "@popperjs/core": "2.6.0",
    "@roxi/routify": "2.15.1",
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "__@comps": "link:./src/components",
    "__@modules": "link:./src/modules",
    "__@root": "link:./src",
    "__@stores": "link:./src/stores",
    "autonumeric": "^4.6.0",
    "crypto-js": "^4.0.0",
    "date-fns": "^2.17.0",
    "dropzone": "^5.7.2",
    "flatpickr": "^4.6.9",
    "graphql-subscriptions-client": "^0.16.0",
    "noty": "^3.2.0-beta",
    "papaparse": "^5.3.0",
    "perfect-scrollbar": "^1.5.0",
    "performant-array-to-tree": "^1.8.1",
    "svelte-file-dropzone": "^0.0.15",
    "svelte-infinite-loading": "^1.3.5",
    "svelte-virtualized-auto-sizer": "^1.0.0",
    "tippy.js": "^6.2.7"
  },
  "devDependencies": {
    "@nurdiansyah/cpx": "1.5.0",
    "@nurdiansyah/devel": "^5.1.0",
    "@nurdiansyah/eslint-config-devel": "^7.1.0",
    "@nurdiansyah/rollup": "^1.3.0",
    "@nurdiansyah/stylelint-rscss": "^1.0.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@types/flatpickr": "^3.1.2",
    "@types/nanoid": "^2.1.0",
    "@types/nconf": "^0.10.0",
    "@types/node": "*",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.8.0",
    "autoprefixer": "^10.2.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "fs-extra": "9.0.1",
    "husky": "^4.3.0",
    "hygen": "^6.0.4",
    "jsdom": "^16.4.0",
    "lint-staged": "^10.5.1",
    "nollup": "^0.14.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.1",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.0",
    "rollup-plugin-hot": "^0.1.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte-hot": "^0.13.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^5.2.1",
    "sass": "1.30.0",
