    "@popperjs/core": "^2.9.2",
    "@roxi/routify": "^2.18.8",
    "autonumeric": "^4.6.0",
    "chart.js": "^3.7.1",
    "cross-fetch": "^3.1.4",
    "d3-array": "^3.1.4",
    "d3-format": "^3.1.0",
    "d3-quadtree": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.1.0",
    "dayjs": "^1.10.5",
