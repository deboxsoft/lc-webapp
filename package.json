{
  "name": "@deboxsoft/lc-webapp",
  "version": "1.0.1",
  "description": "Web Application Learning Center.",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "run-p routify nollup",
    "dev:ssr": "run-p routify rollup",
    "build": "cross-env NODE_ENV=production npm run build:app",
    "build:deploy": "pnpm build && rm -Rf ../../deploy/webapp/dist/build && cp -R ./dist/build ../../deploy/webapp/dist",
    "build:app": "routify -b && rollup -c",
    "build:static": "spank",
    "format": "prettier --write --plugin-search-dir ./ src/**/*.*",
    "serve": "spassr --ssr",
    "rollup": "rollup -cw",
    "nollup": "nollup -c",
    "routify": "routify",
    "lint:eslint": "eslint --fix \"**/*.{ts,tsx,js,jsx}\"",
    "lint:svelte": "svelte-check ./",
    "lint:tsc": "tsc --noEmit",
    "lint": "run-p lint:svelte lint:eslint lint:tsc"
  },
  "dependencies": {
    "@deboxsoft/accounting-api": "^1.5.24",
