    "stylelint-order": "4.1.0",
    "sucrase": "^3.0.0",
    "svelte": "^3.35.0",
    "svelte-check": "^1.2.2",
    "svelte-language-server": "^0.12.18",
    "svelte-preprocess": "^4.6.9",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3",
    "workbox-core": "5.1.4",
    "workbox-expiration": "5.1.4",
    "workbox-precaching": "5.1.4",
    "workbox-routing": "5.1.4",
    "workbox-strategies": "5.1.4"
  }
}
